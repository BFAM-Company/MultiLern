{
  "name": "@netlify/edge-functions",
  "type": "module",
  "version": "2.5.1",
  "description": "TypeScript types for Netlify Edge Functions",
  "main": "node/dist/index.mjs",
  "types": "node/dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./node/dist/index.d.ts",
      "default": "./node/dist/index.mjs"
    },
    "./version": {
      "types": "./node/dist/version.d.mts",
      "default": "./node/dist/version.mjs"
    }
  },
  "devDependencies": {
    "@types/node": "^20.12.4",
    "typescript": "^5.1.6"
  },
  "files": [
    "node/dist/*.mjs",
    "node/dist/**/*.d.*s"
  ],
  "license": "MIT",
  "author": "Netlify Inc.",
  "scripts": {}
}