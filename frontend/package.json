{
  "name": "frontend",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --silent --runInBand",
    "coverage": "npx jest --coverage --silent --runInBand",
    "android:clean": "cd android && ./gradlew clean && cd ..",
    "e2e:build-ios-debug": "npx detox build -c ios.sim.debug",
    "e2e:build-ios-release": "npx detox build -c ios.sim.release",
    "e2e:test-ios-debug": "npx detox test -c ios.sim.debug",
    "e2e:test-ios-release": "npx detox test -c ios.sim.release",
    "e2e:build-android-debug": "yarn android:clean && npx detox build -c android.emu.debug",
    "e2e:build-android-release": "yarn android:clean && detox build -c android.emu.release",
    "e2e:test-android-debug": "detox test -c android.emu.debug",
    "e2e:test-android-release": "detox test -c android.emu.release"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFiles": [
      "./__mocks__/mock-async-storage.ts",
      "@shopify/react-native-skia/jestSetup.js"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ],
    "transformIgnorePatterns": [
      "node_modules/(@shopify/react-native-skia)"
    ],
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    }
  },
  "dependencies": {
    "@babel/preset-env": "^7.24.6",
    "@expo/metro-runtime": "^3.1.1",
    "@expo/vector-icons": "^13.0.0",
    "@hookform/error-message": "^2.0.1",
    "@react-native-async-storage/async-storage": "^1.22.3",
    "@react-native-community/cli-doctor": "^13.6.6",
    "@react-native-community/picker": "^1.8.1",
    "@react-native-picker/picker": "^2.7.6",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@shopify/react-native-skia": "^0.1.239",
    "@types/node": "^20.12.13",
    "@types/react-native-snap-carousel": "^3.8.11",
    "axios": "^1.6.7",
    "axios-auth-refresh": "^3.3.6",
    "axios-mock-adapter": "^1.22.0",
    "detox": "^20.28.0",
    "env": "^0.0.2",
    "expo": "~50.0.2",
    "expo-image-picker": "^15.0.5",
    "expo-linear-gradient": "~12.7.1",
    "expo-status-bar": "~1.11.1",
    "expo-web-browser": "^12.8.2",
    "fuse.js": "^7.0.0",
    "jest": "^29.7.0",
    "jest-expo": "~50.0.4",
    "metro-react-native-babel-preset": "^0.77.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.50.0",
    "react-native": "0.73.2",
    "react-native-cli": "^2.0.1",
    "react-native-debugger": "^1.1.0",
    "react-native-dotenv": "^3.4.11",
    "react-native-gesture-handler": "2.14.0",
    "react-native-modal": "^13.0.1",
    "react-native-paper": "^5.12.3",
    "react-native-picker-select": "^9.1.3",
    "react-native-reanimated": "3.6.2",
    "react-native-safe-area-context": "^4.8.2",
    "react-native-screens": "^3.29.0",
    "react-native-snap-carousel": "^3.9.1",
    "react-native-web": "^0.19.12"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.4.5",
    "@tsconfig/react-native": "^3.0.3",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.2.48",
    "@types/react-test-renderer": "^18.0.7",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "private": true
}
